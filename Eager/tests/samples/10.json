{
    "apiName":"StudentResource",
    "apiVersion":"1.0",
    "swaggerVersion":"1.2",
    "basePath":"/resources",
    "resourcePath":"/students",
    "apis":[
        {
            "path":"/students",
            "description":"",
            "operations":[
                {
                    "method":"POST",
                    "nickname":"addStudent",
                    "type":"Student2",
                    "parameters":[
                        {
                            "paramType":"form",
                            "name":"firstName",
                            "dataType":"string",
                            "required":true
                        },
                        {
                            "paramType":"form",
                            "name":"lastName",
                            "dataType":"string",
                            "required":true
                        }
                    ],
                    "produces":[ "application/json" ],
                    "consumes":[ "application/x-www-form-urlencoded" ],
                    "responseMessages":[
                        {
                            "code":201,
                            "message":"Student resource created"
                        },
                        {
                            "code":500,
                            "message":" Unexpected runtime error",
                            "responseModel":"Error"
                        }
                    ]
                }
            ]
        }
    ],
    "models":{
        "StudentList":{
            "id":"StudentList",
            "properties":{
                "students":{
                    "type":"array",
                    "items":{
                        "$ref":"Student"
                    }
                }
            },
            "required":[ "students" ]
        },
        "Student2":{
            "id":"Student2",
            "properties":{
                "lName":{
                    "type":"string"
                },
                "fName":{
                    "type":"string"
                }
            },
            "required":[ "lastName", "firstName" ]
        },
        "Error":{
            "id":"Error",
            "properties":{
                "reason":{
                    "type":"array",
                    "items":{
                        "$ref":"string"
                    }
                },
                "code":{
                    "type":"integer"
                }
            },
            "required":[ "code" ]
        }
    }
}