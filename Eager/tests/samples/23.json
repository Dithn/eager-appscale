{
    "apiName":"StudentResource",
    "apiVersion":"1.0",
    "swaggerVersion":"1.2",
    "basePath":"/resources",
    "resourcePath":"/students",
    "apis":[
        {
            "path":"/students",
            "description":"",
            "operations":[
                {
                    "method":"POST",
                    "nickname":"addStudent",
                    "type":"boolean",
                    "parameters":[
                        {
                            "paramType":"body",
                            "name":"student",
                            "dataType":"Student",
                            "required":true
                        }
                    ],
                    "produces":[ "application/json" ],
                    "consumes":[ "application/x-www-form-urlencoded" ],
                    "responseMessages":[
                        {
                            "code":201,
                            "message":"Student resource created"
                        },
                        {
                            "code":500,
                            "message":" Unexpected runtime error",
                            "responseModel":"Error"
                        }
                    ]
                }
            ]
        }
    ],
    "models":{
        "StudentList":{
            "id":"StudentList",
            "properties":{
                "students":{
                    "type":"array",
                    "items":{
                        "$ref":"Student"
                    }
                }
            },
            "required":[ "students" ]
        },
        "Student":{
            "id":"Student",
            "properties":{
                "lastName":{
                    "type":"string"
                }
            },
            "required":[ "lastName" ]
        },
        "Error":{
            "id":"Error",
            "properties":{
                "reason":{
                    "type":"array",
                    "items":{
                        "$ref":"string"
                    }
                },
                "code":{
                    "type":"integer"
                }
            },
            "required":[ "code" ]
        }
    }
}