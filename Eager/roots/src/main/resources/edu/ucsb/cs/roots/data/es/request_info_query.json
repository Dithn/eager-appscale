{
  "query": {
    "bool": {
      "must": {"term": {"%s": "%s"}},
      "filter": {
        "range": {
          "%s": {
            "gte": %d,
            "lt": %d,
            "format": "epoch_millis"
          }
        }
      }
    }
  },
  "sort": {
    "timestamp": "asc"
  },
  "size": 4000
}